  "SUPPORT": {
    "TITLE": "Support and Help",
    "FAQS": "Frequently Asked Questions",
    "FAQ_ITEMS": {
      "HOW_TO_COMPLAIN": {
        "QUESTION": "How do I file a new complaint?",
        "ANSWER": "To file a new complaint, go to the \"Complaints\" section in the main menu and click on the \"New Complaint\" button. Complete the form with the required information and click \"Submit\"."
      },
      "VIEW_STATUS": {
        "QUESTION": "How can I view the status of my complaints?",
        "ANSWER": "In the \"Complaints\" section you will find a list of all your complaints with their current status. You can click on any complaint to view detailed information and update history."
      },
      "EDIT_COMPLAINT": {
        "QUESTION": "Can I edit a complaint after submitting it?",
        "ANSWER": "It is only possible to edit a complaint while it is in \"Pending review\" status. Once it has been accepted and changes to \"In process\", it cannot be modified. In that case, contact support for any updates."
      },
      "NOTIFICATION_PREFERENCES": {
        "QUESTION": "How do I change my notification preferences?",
        "ANSWER": "Go to the \"Settings\" section and then to \"Notification Preferences\". There you can enable or disable email and push notifications according to your preferences."
      },
      "FORGOT_PASSWORD": {
        "QUESTION": "What should I do if I forgot my password?",
        "ANSWER": "On the login page, click \"Forgot your password?\". Enter your email address and we will send you a link to reset your password."
      }
    },
